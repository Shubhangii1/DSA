package TEST1;

public class SunOfNNaturalNos {
	
	public static int sumOfNums(int[] arr, int len) {
		int sum =0;
		
		for(int i =0; i<=len; i++) {
			sum = sum + i;
			
		}
		return sum;
		
	}
	
	public static void main(String[] args) {
		int[] arr = {1,2,3,4,5};
		int len = arr.length;
		System.out.println(sumOfNums(arr, len));
		
	}

}
